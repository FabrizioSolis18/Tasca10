a = [3, (1,3), [4, 5, 6], 7, "hola","0", 0, 1] 
if 'a' in a:
    print("a és dins la llista {}".format(a))
elif 'hola' in a:
    print ("holas es a dins la llista {}".format(a))
elif  '0b' in a: 
    print("Aixo s'executa si és certa la condició")
elif 'b' in a:
    print("b és dins {}".format(a))
else: 
    print("dins a hi ha {}".format(a))